# weapon-name: Scyclone

options:
	effect-meter: 5
	damage-amount: 2

function kitEffect_Scyclone(player: player):
	nameItemCT({_player}, "Scyclone")

	if getInvisibleStatus({_player}) is true:
		kitEffect_Scyclone_Cyclone({_player})
		stop
	
	set {_baseloc} to head of {_player}

