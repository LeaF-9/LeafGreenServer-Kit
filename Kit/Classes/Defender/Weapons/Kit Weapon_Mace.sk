# Mace

function kitEffect_Mace_AttackNormal(player: player, event: object):
	set {_victim} to {_event}.getEntity()
	set {_attacker} to {_player}
	{_attacker} is sneaking
	playSound("entity.iron_golem.damage", 1, 1, {_victim})
	kitStun({_victim}, 10)
	loop 10 times:
		set {_el} to aroundEffect({_victim})
		spawnParticle_Block(dirt, {_el})