# Club

function kitEffect_Club_AttackLow(player: player, event: object):
	set {_victim} to {_event}.getEntity()
	set {_attacker} to {_player}
	{_victim}'s facing is {_attacker}'s facing
	setDamageModification_MUL({_victim}, 1.5)
	playSound("entity.iron_golem.damage", 1, 0.5, {_victim})
	loop 10 times:
		set {_el} to aroundEffect({_victim})
		spawnParticle_Block(oak log, {_el})