# Weapon-Name: Fay Stick

function kitEffect_FayStick_AttackLow(player: player, event: object):
	set {_attacker} to {_player}
	kitCheckAlive({_attacker}) is true
	if {_attacker} is sneaking:
		kitHeal({_attacker}, 0.5)
	else:
		{_attacker} does not have regeneration
		applyEffect({_attacker}, regeneration, 1, 60)