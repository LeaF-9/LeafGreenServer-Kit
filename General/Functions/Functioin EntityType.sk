function checkEntityType(entity: entity, type: text) :: boolean:
	# player
	set {_return} to false
	if {_type} is "player":
		if {_entity}.getType() is EntityType.PLAYER:
			set {_return} to true
	# other
	else:
		set {_entitytype} to {_type} parsed as entity type
		if {_entity}.getType() is {_entitytype}:
			set {_return} to true
	return {_return}
