function updateTablist(player: player):
	if {Server} is "Kit":
		updateTablist_Kit({_player})

function updateTablist_Kit(player: player):
	if {Kit-tochu} is true:
		set {_player}'s tab list header to "<##00bfff>§lKit PvP §7- <##00ff00>§lGame going on now!"
		set {_player}'s tab list footer to "<##ffd700>Map§b: <##ffa500>§l%{Kit-GameStage::name}% <##ff0000>Rule§b: <##b22222>§l%{Kit-GameStage::rule}%"
	else:
		set {_player}'s tab list header to "<##00bfff>§lKit PvP §7- <##ff0000>§lGame is not holden"
		set {_player}'s tab list footer to "<##ffd700>Currently Joining§b: <##ffff00>%{Kit-Join}%"